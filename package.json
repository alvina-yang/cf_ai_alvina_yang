    "reactflow": "^11.11.0",
    "zustand": "^4.5.0",
    "@tanstack/react-query": "^5.28.0",
    "lucide-react": "^0.336.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "tailwind-merge": "^2.2.0",
    "sonner": "^1.4.0",
    "framer-motion": "^11.0.0",
    "zod": "^3.22.0",
    "nanoid": "^5.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "@types/node": "^20.11.0",
    "@types/react": "^18.3.0",
